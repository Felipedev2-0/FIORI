            <Button id="_IDGenButton2" text="Cadastro de Produtos Indiretos" press="onEditCustomer1" class="button-spacing large-buttons"/>
            <Button id="_IDGenButton3" text="Cadastro de Fornecedor" press="onEditCustomer1" class="button-spacing large-buttons"/>
