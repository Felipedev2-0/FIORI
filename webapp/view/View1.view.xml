<mvc:View
    controllerName="zov.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns:t="sap.ui.table"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <VBox id="_IDGenVBox1" class="sapUiTinyMargin" alignItems="Center">
            <Button id="_IDGenButton1" text="Cadastro de Produtos" press="onNewCustomer" class="button-spacing"/>
            <Button id="_IDGenButton2" text="Cadastro de Fornecedor" press="onEditCustomer1" class="button-spacing"/>
            <Button id="_IDGenButton3" text="Cadastro de Colaborades" press="onEditCustomer1" class="button-spacing"/>
            <Button id="_IDGenButton4" text="Escrituração Fiscal " press="onEditCustomer1" class="button-spacing"/>
        </VBox>
    </Page>
</mvc:View>
